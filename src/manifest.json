{
  "manifest_version": 3,
  "version": "1.1.3",
  "name": "WebPageTest Recorder Extension",
  "description": "An extension for Chrome Recorder to export a recorder session as a WebPageTest Custom Script",
  "permissions": [],
  "devtools_page": "Webpagetest-export.html",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "action": {
    "default_popup": "Webpagetest-export.html",
    "default_title": "WebPageTest Recorder Extention"
  },
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "minimum_chrome_version": "104.0.0.0"
}